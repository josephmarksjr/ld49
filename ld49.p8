pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- jam: ludum dare 49
-- title: unstable
-- author: joseph marks
-- date: october 2021
---------------------------
-- collect the star

offset=0
px=0
py=0
dy=0
dx=0
ground=false
dc=0//drill cell for animation
txt="hello"//debug text
bs={}
intro=true
gameover=false
lvl=1
lives=3
timer=3

function init_block(t,x,y)
 b={}
 b.x=x
 b.y=y
 b.t=t
 b.falling=false
 b.fcount=0
 b.dy=0
 b.dx=0
 return b
end

function init_game()
 gameover=false
 intro=true
 lives=3
 init_level(1)
end

function init_level(n)
 bs={}
 add(bs,init_block(48,6*8,49*8))
 add(bs,init_block(48,7*8,49*8))
 add(bs,init_block(48,8*8,49*8))
 add(bs,init_block(48,9*8,49*8))
 add(bs,init_block(48,7*8,41*8))
 add(bs,init_block(48,8*8,41*8))
 offset=100
 px=4*8
 py=59*8
 dx=0
 dy=0
 ground=false
end

function _init()
 init_level(1)
end

function _update60()
 if(timer>0)then
  timer-=1/60
 end
 
 if(gameover==true)then
  if(btnp(0) or btnp(1) or
   btnp(2) or btnp(3) or
   btnp(4) or btnp(5))
  then
   init_game()
  end
 elseif(intro==true)then
  if(btnp(0) or btnp(1) or
   btnp(2) or btnp(3) or
   btnp(4) or btnp(5))
  then
   intro=false
  end
 else
 
 if(timer<0)then
  offset+=0.3
  if(offset<-127)then
   offset=0
  end
 end
 
 --gravity
 dy=dy+0.06
 
 --left
 if(btn(0))then
  dx=dx-0.05
 end
 
 --right
 if(btn(1))then
  dx=dx+0.05
 end
 
 --jump
 if(btnp(2))then
  if(ground)then
   ground=false
   dy=dy-2
  end
 end
 
 --fall faster
 if(btn(3))then
  dy=dy+0.05
 end
 
 --l/r collisions
 if(hit(px+dx,py,7,7))then
  dx=0
 end
 
 --u/d collisions
 if(hit(px,py+dy,7,7))then
  if(dy>0)then
   ground=true
  end
  dy=0
 end
 
 --b collisions
 for o in all(bs) do
  if(
   hit_o(px,py+dy,7,7,o.x,o.y)
  )then
    if(dy>0)then
     ground=true
    end
    dy=0
    o.falling=true
  end
  if(
  	hit_o(px+dx,py,7,7,o.x,o.y)
  )then
   dx=0
  end
 end
 
 if(ground)then
  dx=dx*0.90
  jump=0
 end
 
 --prevent going too fast
 if(dx>1)then
  dx=1
 end
 if(dx<-1)then
  dx=-1
 end
 if(dy>2)then
  dy=2 
 end
 if(dy<-2)then
  dy=-2
 end
 
 if(dc>=2)then
  dc=0
 else
  dc+=0.1
 end
 
 for b in all(bs) do 
  if(b.y>594)then
   del(bs,b)
  elseif(b.falling==true)then
   b.fcount+=1
   if(b.fcount>3)then
    b.dy=b.dy+0.01
   end
  end
  b.y=b.y+b.dy
 end
 
 if((py+offset)>594)then
  die()
 end
 
 py=py+dy
 px=px+dx
 end
end

function hit_star(px,py,sx,sy)
 if(px>sx+8)then
  return false
 end
 if(px<sx-8)then
  return false
 end
 if(py>sy+8)then
  return false
 end
 if(py<sy-8)then
  return false
 end
 return true
end

function _draw()
  if(gameover==true)then
   cls()
   camera(0,0)
   draw_gameover()
  elseif(intro==true)then
   cls()
   camera(0,0)
   draw_lvl_intro(1)
  else
		 cls()
		 map(0,0,0,0,16,64)
		 camera(0,488-offset)
		 spr(32,px,py)
		 for i=0,16 do
		  spr(17,i*8,608-offset)
		  spr(dc+2,i*8,600-offset)
		 end 
		 --spr(1,sx,sy)
		
		 foreach(bs,draw_b)
		
		 --print(txt,px+10,py-30)
		end
end

function draw_b(b)
 spr(b.t,b.x,b.y)
end

function die()
 lives-=1
 if(lives<=0)then
  gameover=true
 else
  intro=true
  init_level(1)
 end
end

function hit_o(x,y,w,h,ox,oy)
 if(x>ox+8)then
  return false
 end
 if(x<ox)then
  return false
 end
 if(y>oy+8)then
  return false
 end
 if(y+7<oy)then
  return false
 end
 
 return true
end

function hit(x,y,w,h,t)
 collide=false
 for i=x,x+w,w do
 	local a=fget(mget(i/8,y/8))
 	local b=fget(mget(i/8,(y+h)/8))
  if(a>0)then
   collide=true
  end
  if(b>0)then
   collide=true
  end
 end
 
 for i=y,y+h,h do
  if
   (fget(mget(x/8,i/8))>0) or
   (fget(mget((x+w)/8,i/8))>0)
  then
   collide=true
  end
 end
 
 return collide
end

function draw_lvl_intro(l)
 print("level ".. l,50,20)
 print("lives: "..lives,48,28)
 print("press any button to play",16,36)
end

function draw_gameover(l)
 print("game over",46,20)
 print("press any button to play",16,28)
end
